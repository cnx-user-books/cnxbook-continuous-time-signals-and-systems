    </m:mtr>
    <m:mtr>
      <m:mtd>
        <m:mi>Y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>s</m:mi>
        <m:mo stretchy="false">)</m:mo>
