          <m:mi>d</m:mi>
          <m:mrow>
            <m:mi>d</m:mi>
            <m:mi>t</m:mi>
          </m:mrow>
        </m:mfrac>
        <m:mi>y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>t</m:mi>
        <m:mo stretchy="false">)</m:mo>
        <m:mo>+</m:mo>
        <m:mn>2</m:mn>
        <m:mi>y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>t</m:mi>
        <m:mo stretchy="false">)</m:mo>
        <m:mo fence="false" stretchy="false">}</m:mo>
      </m:mtd>
      <m:mtd>
        <m:mi/>
        <m:mo>=</m:mo>
        <m:mrow class="MJX-TeXAtom-ORD">
