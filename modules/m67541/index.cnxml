            <m:mi>t</m:mi>
          </m:mrow>
        </m:mfrac>
        <m:mi>y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>t</m:mi>
        <m:mo stretchy="false">)</m:mo>
        <m:mo>+</m:mo>
        <m:mn>2</m:mn>
        <m:mi>y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>t</m:mi>
        <m:mo stretchy="false">)</m:mo>
      </m:mtd>
      <m:mtd>
        <m:mi/>
        <m:mo>=</m:mo>
        <m:mi>x</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>t</m:mi>
        <m:mo stretchy="false">)</m:mo>
      </m:mtd>
    </m:mtr>
    <m:mtr>
      <m:mtd>
        <m:mrow class="MJX-TeXAtom-ORD">
          <m:mi class="MJX-tex-caligraphic" mathvariant="script">L</m:mi>
        </m:mrow>
        <m:mo fence="false" stretchy="false">{</m:mo>
        <m:mfrac>
          <m:mi>d</m:mi>
          <m:mrow>
            <m:mi>d</m:mi>
            <m:mi>t</m:mi>
          </m:mrow>
        </m:mfrac>
        <m:mi>y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>t</m:mi>
        <m:mo stretchy="false">)</m:mo>
        <m:mo>+</m:mo>
        <m:mn>2</m:mn>
        <m:mi>y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>t</m:mi>
        <m:mo stretchy="false">)</m:mo>
        <m:mo fence="false" stretchy="false">}</m:mo>
      </m:mtd>
      <m:mtd>
        <m:mi/>
        <m:mo>=</m:mo>
        <m:mrow class="MJX-TeXAtom-ORD">
          <m:mi class="MJX-tex-caligraphic" mathvariant="script">L</m:mi>
        </m:mrow>
        <m:mo fence="false" stretchy="false">{</m:mo>
        <m:mi>x</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>t</m:mi>
        <m:mo stretchy="false">)</m:mo>
        <m:mo fence="false" stretchy="false">}</m:mo>
      </m:mtd>
    </m:mtr>
    <m:mtr>
      <m:mtd>
        <m:mi>s</m:mi>
        <m:mi>Y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>s</m:mi>
        <m:mo stretchy="false">)</m:mo>
        <m:mo>+</m:mo>
        <m:mn>2</m:mn>
        <m:mi>Y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>s</m:mi>
        <m:mo stretchy="false">)</m:mo>
      </m:mtd>
      <m:mtd>
        <m:mi/>
        <m:mo>=</m:mo>
        <m:mi>X</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>s</m:mi>
        <m:mo stretchy="false">)</m:mo>
      </m:mtd>
    </m:mtr>
    <m:mtr>
      <m:mtd>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>s</m:mi>
        <m:mo>+</m:mo>
        <m:mn>2</m:mn>
        <m:mo stretchy="false">)</m:mo>
        <m:mi>Y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>s</m:mi>
        <m:mo stretchy="false">)</m:mo>
      </m:mtd>
      <m:mtd>
        <m:mi/>
        <m:mo>=</m:mo>
        <m:mi>X</m:mi>
        <m:mo stretchy="false">(</m:mo>
