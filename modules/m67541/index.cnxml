        <m:mo>+</m:mo>
        <m:mn>2</m:mn>
        <m:mo stretchy="false">)</m:mo>
        <m:mo>+</m:mo>
        <m:mi>B</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>s</m:mi>
        <m:mo>+</m:mo>
        <m:mn>3</m:mn>
        <m:mo stretchy="false">)</m:mo>
      </m:mtd>
    </m:mtr>
    <m:mtr>
      <m:mtd>
        <m:mn>1</m:mn>
      </m:mtd>
      <m:mtd>
        <m:mi/>
        <m:mo>=</m:mo>
        <m:mi>s</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>A</m:mi>
        <m:mo>+</m:mo>
        <m:mi>B</m:mi>
        <m:mo stretchy="false">)</m:mo>
        <m:mo>+</m:mo>
        <m:mo stretchy="false">(</m:mo>
        <m:mn>2</m:mn>
        <m:mi>A</m:mi>
        <m:mo>+</m:mo>
        <m:mn>3</m:mn>
        <m:mi>B</m:mi>
        <m:mo stretchy="false">)</m:mo>
