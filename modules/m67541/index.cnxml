        <m:mo>+</m:mo>
        <m:mn>2</m:mn>
        <m:mo stretchy="false">)</m:mo>
        <m:mi>Y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>s</m:mi>
        <m:mo stretchy="false">)</m:mo>
      </m:mtd>
      <m:mtd>
        <m:mi/>
        <m:mo>=</m:mo>
        <m:mfrac>
          <m:mn>1</m:mn>
          <m:mrow>
            <m:mi>s</m:mi>
            <m:mo>+</m:mo>
            <m:mn>3</m:mn>
          </m:mrow>
        </m:mfrac>
      </m:mtd>
    </m:mtr>
    <m:mtr>
      <m:mtd>
        <m:mi>Y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>s</m:mi>
        <m:mo stretchy="false">)</m:mo>
      </m:mtd>
      <m:mtd>
        <m:mi/>
        <m:mo>=</m:mo>
        <m:mfrac>
          <m:mn>1</m:mn>
          <m:mrow>
            <m:mo stretchy="false">(</m:mo>
            <m:mi>s</m:mi>
            <m:mo>+</m:mo>
            <m:mn>3</m:mn>
            <m:mo stretchy="false">)</m:mo>
            <m:mo stretchy="false">(</m:mo>
            <m:mi>s</m:mi>
            <m:mo>+</m:mo>
            <m:mn>2</m:mn>
            <m:mo stretchy="false">)</m:mo>
          </m:mrow>
        </m:mfrac>
      </m:mtd>
    </m:mtr>
    <m:mtr>
