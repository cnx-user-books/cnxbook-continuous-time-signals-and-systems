        </m:mrow>
        <m:mo fence="false" stretchy="false">{</m:mo>
        <m:msup>
          <m:mi>e</m:mi>
          <m:mrow class="MJX-TeXAtom-ORD">
            <m:mo>−</m:mo>
            <m:mn>3</m:mn>
            <m:mi>t</m:mi>
          </m:mrow>
        </m:msup>
        <m:mi>u</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>t</m:mi>
        <m:mo stretchy="false">)</m:mo>
        <m:mo fence="false" stretchy="false">}</m:mo>
      </m:mtd>
    </m:mtr>
    <m:mtr>
      <m:mtd>
        <m:mi>s</m:mi>
        <m:mi>Y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>s</m:mi>
        <m:mo stretchy="false">)</m:mo>
        <m:mo>+</m:mo>
        <m:mn>2</m:mn>
        <m:mi>Y</m:mi>
        <m:mo stretchy="false">(</m:mo>
        <m:mi>s</m:mi>
        <m:mo stretchy="false">)</m:mo>
      </m:mtd>
      <m:mtd>
        <m:mi/>
        <m:mo>=</m:mo>
        <m:mfrac>
          <m:mn>1</m:mn>
          <m:mrow>
            <m:mi>s</m:mi>
            <m:mo>+</m:mo>
