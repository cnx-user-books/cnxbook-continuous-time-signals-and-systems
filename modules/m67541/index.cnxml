    <m:mtr>
      <m:mtd>
        <m:mn>1</m:mn>
      </m:mtd>
      <m:mtd>
        <m:mi/>
        <m:mo>=</m:mo>
        <m:mi>A</m:mi>
        <m:mo stretchy="false">(</m:mo>
