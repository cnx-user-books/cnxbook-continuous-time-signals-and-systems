    </m:mtr>
    <m:mtr>
      <m:mtd>
        <m:mi>A</m:mi>
        <m:mo>+</m:mo>
        <m:mi>B</m:mi>
        <m:mo>=</m:mo>
        <m:mn>0</m:mn>
        <m:mtext> </m:mtext>
        <m:mo>,</m:mo>
        <m:mtext> </m:mtext>
        <m:mn>2</m:mn>
        <m:mi>A</m:mi>
        <m:mo>+</m:mo>
        <m:mn>3</m:mn>
        <m:mi>B</m:mi>
        <m:mo>=</m:mo>
        <m:mn>1</m:mn>
      </m:mtd>
      <m:mtd>
        <m:mi/>
        <m:mo stretchy="false">→</m:mo>
        <m:mi>A</m:mi>
        <m:mo>=</m:mo>
        <m:mo>−</m:mo>
        <m:mn>1</m:mn>
        <m:mo>,</m:mo>
        <m:mi>B</m:mi>
